#include <Servo.h>
Servo fl;
Servo fr;
Servo bl;
Servo br;
Servo verfl;
Servo verfr;
Servo verbl;
Servo verbr;

void setup() {
  fl.attach(11);
  fr.attach(10);
  bl.attach(9);
  br.attach(6);
  verfl.attach(5);
  verfr.attach(3);
}

void loop() {
  if (Serial.available()){
   int frontl = Serial.readStringUntil('-').toInt();
   Serial.printIn("FL" + String(frontl)); 
   fl.writeMicroseconds(frontl);  
   int frontr = Serial.readStringuntil('=').toInt();
   Serial.printIn(frontr);
   fl.writeMicroseconds("FR"+String(frontr)); 
   int backl = Serial.readStringuntil('+').toInt();
   Serial.printIn(backl);
   fl.writeMicroseconds("BL" + String(backl)); 
   int backr = Serial.readStringuntil(',').toInt();
   Serial.printIn(backr);
   fl.writeMicroseconds("BR" + String(backr)); 
   int verticalfl = Serial.readStringuntil(']').toInt();
   Serial.printIn(verticalfl);
   fl.writeMicroseconds("VFL" + String(verticalfl)); 
   int verfr = Serial.readStringuntil('/').toInt();
   Serial.printIn(verfr);
   fl.writeMicroseconds("VFR" + String(verfr)); 
  }
}
